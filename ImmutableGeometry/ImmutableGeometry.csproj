<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <LangVersion>9</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>Gravy.ImmutableGeometry</PackageId>
        <PackageVersion>0.1.0-alpha</PackageVersion>
        <Title>Gravy.ImmutableGeometry</Title>
        <Authors>Kevin Gravier</Authors>
        <Description>An immutable int based Geomtery implementation.</Description>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <Copyright>Kevin Gravier 2021</Copyright>
        <PackageProjectUrl>https://github.com/mrkmg/ImmutableGeometry</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/mrkmg/ImmutableGeometry.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>immutable, point, vector, geometry, int</PackageTags>
    </PropertyGroup>

</Project>
